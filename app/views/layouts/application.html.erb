<!DOCTYPE html>
<html>
<head>
  <title>Flatwhite</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render 'common/header' %>
<div class="container">
	<div class ="row">
		<div class="span8">
			<%= yield %>
		</div>
		<div class="span4">
			<%= render 'common/sidebar' %>
		</div>
	</div>
</div>
<%= render 'common/footer' %>
</body>
</html>
